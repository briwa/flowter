{
  "name": "@flowter/flowchart",
  "description": "A simple flowchart component made with Vue. Requires a graph-like data structure as an input and renders it as a flowchart.",
  "version": "0.2.2",
  "keywords": [
    "flowchart",
    "typescript",
    "vue",
    "vuejs",
    "diagram",
    "component",
    "vue-component"
  ],
  "author": "https://github.com/briwa",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:static": "vue-cli-service build",
    "build:library": "vue-cli-service build --target lib --name flowter src/components/flowter-flowchart/index.vue",
    "lint": "vue-cli-service lint",
    "test": "vue-cli-service test:unit",
    "test:ci": "vue-cli-service test:unit --ci",
    "preversion": "npm run lint && npm run test",
    "version": "npm run build:library",
    "postversion": "git push && git push --tags",
    "docs": "rimraf docs && typedoc src"
  },
  "main": "dist/flowter.common.js",
  "files": [
    "dist/flowter.common.js",
    "dist/flowter.umd.js",
    "dist/flowter.css"
  ],
  "dependencies": {
    "vue": "2.6.6",
    "vue-class-component": "6.0.0",
    "vue-property-decorator": "7.0.0"
  },
  "devDependencies": {
    "@types/jest": "23.1.4",
    "@vue/cli-plugin-babel": "3.5.0",
    "@vue/cli-plugin-typescript": "3.5.0",
    "@vue/cli-plugin-unit-jest": "3.8.0",
    "@vue/cli-service": "3.7.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "ts-jest": "23.10.0",
    "typedoc": "0.14.2",
    "typescript": "3.2.1",
    "vue-template-compiler": "2.6.6"
  },
  "license": "MIT",
  "repository": "https://github.com/briwa/flowter"
}
