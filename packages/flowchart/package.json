{
  "name": "@flowter/flowchart",
  "description": "A simple flowchart component made with Vue. Requires a graph-like data structure as an input and renders it as a flowchart.",
  "version": "0.1.3",
  "keywords": [
    "flowchart",
    "typescript",
    "vue",
    "vuejs",
    "diagram",
    "component",
    "vue-component"
  ],
  "scripts": {
    "serve": "vue-cli-service serve ./serve.ts",
    "build": "vue-cli-service build --target lib --name @flowter/flowchart src/index.vue"
  },
  "main": "dist/@flowter/flowchart.common.js",
  "files": [
    "dist/@flowter/flowchart.common.js",
    "dist/@flowter/flowchart.umd.js",
    "dist/@flowter/flowchart.css"
  ],
  "dependencies": {
    "@flowter/constants": "^0.1.2",
    "@flowter/edge": "^0.1.2",
    "@flowter/node": "^0.1.2",
    "@flowter/types": "^0.1.2",
    "vue": "2.6.6",
    "vue-class-component": "6.0.0",
    "vue-property-decorator": "7.0.0"
  },
  "devDependencies": {
    "@types/jest": "23.1.4",
    "@vue/cli-plugin-babel": "3.5.0",
    "@vue/cli-plugin-typescript": "3.5.0",
    "@vue/cli-plugin-unit-jest": "3.8.0",
    "@vue/cli-service": "3.7.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "node-sass": "4.12.0",
    "sass-loader": "8.0.0",
    "ts-jest": "23.10.0",
    "typedoc": "0.14.2",
    "typescript": "3.6.4",
    "vue-template-compiler": "2.6.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "briwa <ssbriwa@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/briwa/flowter"
